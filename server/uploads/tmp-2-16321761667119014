<script>
    function getAddWarrantyPageV1(price, symbol) {
        var warranty_url = 'https://www.ubuy.com.bd/en/ubcommon/add-warranty-v4';
        var membership_item = true;
        var customer_id = '';
        var entity_id = '81UQ85HU8';
        var token = 'ODF1cTg1aHU4NzB8YThjOTNmMTNkYTYwNzM4MDJmYmUyNTg2NWViYWE4MzA3MzlkMGI0MzMwZjZlZDZhYjE0YWFkMjFiNDhkYjMyYXwxYjRhMDNlZGM4MzQyMTkxMmQzYWUxODMyNzYzZjM5MTA3YWUzOWRlMTk4NTI1YWEwNWQyZjQyZWFkODIzN2Vh';
        var screen_size = {};
        screen_size.height = window.innerHeight;
        screen_size.width = window.innerWidth;
        if (screen_size.width <= 1180) {
            warranty_url = 'https://www.ubuy.com.bd/en/ubcommon/add-warranty-mobile-v4';
        }

        jQuery.ajax({
            type: "GET",
            url: warranty_url,
            data: {
                price: price,
                symbol: symbol,
                membership_item:membership_item,
                entity_id: entity_id,
                token: token,
                customer_id:customer_id
            },
            success: function (resp) {
                jQuery(".select-warranty-options").after(resp);
                jQuery(".select-warranty-options").find('#selected_plan').find(".span_spinner").remove();
                jQuery(".select-warranty-options").find('#selected_plan').removeClass('loading');
            },
        });
    }
</script>
<script>
    var continue_shopping_txt = 'Continue Shopping';
    var view_cart_txt = 'View Cart & Checkout';
    var p_price = 0.00;
    var p_currency_symbol = 'BDT';
    var p_currency_iso_symbol = 'BDT';
    var is_fb_viewcontent = false;
    function track_fbq_product(product_id, price, brand) {
        var web_product_id = product_id;
        var p_title = jQuery("#product_name").val();
        var p_image = jQuery("#product_image").val();
        var meta_html = getOgMeta({
            'og:title': p_title,
            'og:description': p_title,
            'og:url': 'https://www.ubuy.com.bd/en/product/81UQ85HU8-restored-apple-iphone-14-pro-256gb-space-black-unlocked-mq0n3ll-a-used-excellent-condition?srsltid=AfmBOorHwlM-KB27kT2IKc8PNS1bJ_K9GKjrR2DlmM9BQYzvSw_s-JhV',
            'og:image': p_image,
            'product:brand': brand,
            'product:availability': "in stock",
            'product:condition': "new",
            'product:price:amount': price,
            'product:price:currency': 'BDT',
            'product:retailer_item_id': web_product_id,
            'og:type': 'product'

        });
        p_price = price;

        jQuery("head").append(meta_html);
        FBViewContent(web_product_id, 1);
        jQuery("#show_currency_amount").val(price);

            }

    function FBViewContent(web_product_id, i) {
        //console.log('fbq',i);
        if (i > 5)
            return;
        try {
            var eventID = "event-" + Date.now() + "-" + Math.floor(Math.random() * 1000000);
            sessionStorage.setItem("fb_event_id", eventID);
            fbq('track', 'ViewContent', {
                content_ids: [web_product_id],
                content_type: 'product',
                value: p_price,
                currency: p_currency_symbol,
            }, {eventID: eventID});
        } catch (error) {
            i++;
            setTimeout(function() {
                FBViewContent(web_product_id, i);
            }, 2000);
        }
    }

    function getOgMeta(og_meta_obj) {
        var meta_html = '';
        jQuery.each(og_meta_obj, function(key, val) {
            meta_html += '<meta property="' + key + '" content="' + val + '">';
        });
        return meta_html;
    }

    function checkRestrictedProduct(data) {
        var utmSourceCustm = jQuery("#utmSourceCustm").val();
        var requestParams = 'type=p_r&';
        jQuery.each(data, function (key, value) {
            if (key != 'length'){
                var param = value;
                if(key == "pnm"){
                    //remove special char from value as given ",',/,(,),[,],\
                    param = decodeURIComponent(param);
                    param = param.replace(/[\'\"\/\\[\](){}]/g, '');
                    param = encodeURIComponent(param);
                }
                requestParams += key + "=" + param + "&";
            }
        });
        requestParams += "ut_cs_src=" + utmSourceCustm;
        var requestUrl = "https://www.ubuy.com.bd/en/ubcommon/check-common-details";

        jQuery.ajax({
            type: 'GET',
            data: requestParams,
            url: requestUrl,
            dataType: 'JSON',
            headers: {
                'Cache-Control': 'max-age=10'
            },
            success: function (data) {
                var totalCount = parseInt(data);
                if (totalCount >= 1) {
                    var showMessage = "<div class='note-danger text-danger'>This product is restricted in your country.</div>";
                    //                    jQuery("#product-variations").remove();
                    //                    jQuery("#product-share-links").remove();
                    jQuery("div.prime-text").remove();
                    //                    jQuery("#product-view-full hr").remove();
                    //                    jQuery(".add-to-cart").html('');
                    //                    jQuery(".availability").parent().remove();
                   
                    jQuery("#product-variations").remove();
                    //jQuery("#product_avg_rating").remove();
                    jQuery("#review_form_container").remove();
                    //jQuery("#customer-rating").remove();
                    jQuery(".bottom-content .shipping_discount").remove();
                    jQuery(".bottom-content .seller-info").remove();                   
                    jQuery(".middle-content").remove();
                    //jQuery(".single-product-description").html(showMessage);
                    //jQuery(".single-product-content").html(showMessage);
                    jQuery(".card-body .product-details").remove();
                    jQuery(".buy-action-sec").remove();
                    jQuery(".product-shop").html('');
                    if(jQuery("#availability_sec").length){
                        jQuery("#availability_sec").html(showMessage);
                    }else{
                        jQuery(".product-shop").after(showMessage);
                    }
                    
                    jQuery("#remove_restriction_button").show();
                    if (jQuery("#notify-in-stock-form").length) {
                        jQuery("#notify-in-stock-form").addClass("d-none");
                    }
                } else {
                    jQuery("#remove_restriction_button").hide();
                }
            }
        });
    }   
</script>
<script>
   function checkUnlistedBrand() {
        var brand_name = jQuery("#brand_name_en").val();
        
        if (typeof brand_name == 'undefined') {
            setTimeout(() => {   
                checkUnlistedBrand(); 
            },1000); 
            return;
        } 
        if(brand_name != '' && typeof brand_name != 'undefined') {            
            var requestUrl = "https://www.ubuy.com.bd/en/ubcommon/save-unlisted-brand/"; 
            requestParams = 'brand=' + encodeURIComponent(brand_name);  
            jQuery.ajax({
                type: 'POST',
                data: requestParams,
                url: requestUrl,
                dataType: 'JSON',
                headers: {
                    'Cache-Control': 'max-age=10'
                },
                success: function (data) {
                    
                }
            });
        }  
    }
    </script>
<script>
    jQuery(window).scroll(function () {
        var bundle = jQuery(".bundle-container").length;
        var instock = jQuery("#availability-status").hasClass("in-stock");
        if (jQuery(window).scrollTop() > 1200 && jQuery(window).width() > 992 && instock && !bundle) {
            jQuery('#product_addtocart_form .actions').addClass('fixed-add-to-cart');
            isSticky = true;
        } else {
            jQuery('#product_addtocart_form .actions').removeClass('fixed-add-to-cart');
            isSticky = false;
        }
    });
</script>

