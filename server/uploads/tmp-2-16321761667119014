            url: other_viewed_url,
            dataType: "html",
            success: function(response) {
                jQuery("#" + other_id + " .u-loader-outer").fadeOut();
                jQuery("#" + other_id + " .u-loader").removeClass("loading");
                jQuery("#" + other_id).removeClass("u-loader-sl");
                jQuery("#" + other_id).removeClass("s1x");
                jQuery("#" + other_id).html(response);
                otherViewedProductCrowsel(other_id);
            }
        });
    }

