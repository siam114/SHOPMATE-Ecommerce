    function fetchDetail() {
        scrollToDiv('body');
        var fetch_data = {
            cur_url: current_url,
            id: entity_id,
            sku: selected_asin,
            v_sku: v_sku,
            p_sku: parent_asin,
            c_id: is_custom_id_product,
            sname: storename,
            sbname: substorename,
            is_ajax: is_ajax,
            pid: product_id,
            sid: s_id,           
            can_url: canonical_url,
            lng: lang,
            dir: dir,
            fn: fn,
            d: device,
            token: csrftoken_fetch,
            lv: lv, 
            customer_id: customer_id,     
            debugg_psr: debugg_psr,
            selected_variation: selected_variation,
            url_key, url_key,
            c_u_p_s_id:custom_url_product_section_id,
            is_one_search: is_one_search,
        };
        if (debugpsr != "") {
            fetch_data.debugpsr = debugpsr;
        }
        if(cur != ""){
            fetch_data.cur = cur;
        }
        if(is_deal_active == true){
            fetch_data.is_deal_active = is_deal_active;
        }
                fetch_data = btoa(unescape(encodeURIComponent(JSON.stringify(fetch_data))));
        jQuery.ajax({
            type: "GET",
            headers: {
                'Cache-Control': 'max-age=10'
            },
            url: ajax_url,            
            data: {
                fetch_data: fetch_data
            },
            timeout: 60000,  // 60 seconds timeout
            success: function (response, textStatus, jqXHR) {
                if (blank == "1") {
                    return false;
                }
                if (jqXHR.status === 204) {
                    window.location.reload();
                }
                jQuery("#product-view-full").attr('data', entity_id);
                if (response) {
                    jQuery('#product-view-full').html(response);
                    if (jQuery('.skeleton-important-info').length) {
                        jQuery('.skeleton-important-info').remove();
                    }                    
                } else {
                    window.location.reload();
                }
            },error: function (jqXHR, textStatus, errorThrown) {
                if (textStatus === "timeout" && hit_count == 1) {
                    hit_count = 2;
                    fetchDetail(); // Retry on timeout
                }
                if (jqXHR.status === 500 && hit_count == 1) {
                    hit_count = 2;
                    fetchDetail(); // Retry on error
                }
            }
        });
    }

    fetchDetail();

    function extractNumbersFromString(str) {       
        return str.match(/[\d.]+/g).map(Number);
    }

    function getStockStatusByPrice(price_usd, store = ''){
        var website_id = '70';
        website_id = parseInt(website_id);
        if(store == ''){
            store = 'US';
        }
        store = store.toUpperCase();

        if (['US'].includes(store) && ![8, 89, 13].includes(website_id)) {
            return true;
        } else if (website_id === 13 && ['US'].includes(store) && price_usd >= 3000) {
            return false;
        }
        if (['DE', 'EU'].includes(store) && price_usd >= 1090) {
            return false;
        }
        if (['IN'].includes(store) && price_usd >= 285) {
            return false;
        }
        if (['CH'].includes(store) && price_usd >= 719) {
            return false;
        }

        if (price_usd >= 2400 && website_id !== 1) {
            return false;
        } else if (price_usd >= 160 && website_id === 8) {
            return false;
        } else if (price_usd >= 500 && website_id === 89) {
            return false;
        } else if (price_usd >= 30 && website_id === 8) {
            return false;
        }

        return true;
    }

    function getWeightFromAI(showInDescription='', lang_code=''){
        var product_id = jQuery("#p_full_id").val();
        if (product_id == '') {
            setTimeout(function() {
                getWeightFromAI(showInDescription, lang_code);
            }, 1000);
            return;
        }
       
        var product_name = jQuery("#product_name").val();      
        var product_description = jQuery("#product-full-desc").find('#description').text();         
        var sku = jQuery("#asin").val();
                // var weight_dime_data = {
        //     product_name: product_name,           
        //     product_id: product_id,
        //     selected_asin: sku
        // };
        //weight_dime_data_enc = btoa(JSON.stringify(weight_dime_data));
        jQuery.ajax({
            type: "POST",
            data: {                
                product_name: product_name,
                product_description:product_description,           
                product_id: product_id,
                lang_code: lang_code,
                selected_asin: sku
            },
            url: aiProductDimensionsWeight_url,
            dataType: "html",
            success: function (response) {

                var weight_data = JSON.parse(response);     
                
                if(typeof weight_data['product_weight'] !== "undefined" && weight_data['product_weight'] !=""){                    
                    full_weight = "";
                    full_dimension = "";
                    product_weight_str = "";
                    if(typeof weight_data['product_weight_str'] !== "undefined" && weight_data['product_weight_str'] != ""){     
                        product_weight_str = weight_data['product_weight_str'];
                    }

                    product_weight = weight_data['product_weight'];
                    product_weight_kg = product_weight * 0.45;
                    product_weight_kg = Math.round(product_weight_kg * 100) / 100;
                    full_weight = product_weight+' lbs('+product_weight_kg+' kg)';   

                    product_dimension = weight_data['product_dimension'];
                    if (product_dimension.trim() !== '' && product_dimension.toLowerCase().indexOf('x') !== -1) {
                        //let dymsArr = product_dimension.split('x'); 
                        let dymsArr = extractNumbersFromString(product_dimension);                   
                        if (dymsArr.length >= 3) {   
                            let rValueTmp = ` (${(dymsArr[0] * 2.54).toFixed(1)} x ${(dymsArr[1] * 2.54).toFixed(1)} x ${(dymsArr[2] * 2.54).toFixed(1)} cms)`;
                            full_dimension = product_dimension+ ' inches'+ rValueTmp;
                        }                        
                    } 
                             
                    if(showInDescription){
                        if(jQuery('.w_d_desciption').length && full_dimension !="" && full_weight !=""){                            
                            if(jQuery('.d_desciption').text().length < 2){
                                jQuery('.d_desciption').html(full_dimension);
                            }
                            if(jQuery('.w_desciption').text().length < 2){
                                jQuery('.w_desciption').html(full_weight);
                            }  
                        }
                    }else{
                        jQuery("#p_weight").val(weight_data['product_weight']);
                        jQuery("#ai_p_weight").val(weight_data['product_weight']);

                        if(jQuery('#product-attribute-specs-table').length){
                            if(lang_code != '' && lang_code == 'en' && !jQuery("#product-attribute-specs-table td:contains('Item Weight')").length){
                                jQuery('#product-attribute-specs-table').append('<tr> <td>Item Weight</td> <td>'+weight_data['product_weight']+' lbs('+product_weight_kg+' kg) </td> </tr>');
                            }else if (!jQuery("#product-attribute-specs-table td:contains('" + item_weight_text + "')").length) {
                                if(product_weight_str != ""){
                                    jQuery('#product-attribute-specs-table').append('<tr> <td>'+item_weight_text+'</td> <td>'+ product_weight_str + ' </td> </tr>');
                                }else{
                                    jQuery('#product-attribute-specs-table').append('<tr> <td>'+item_weight_text+'</td> <td>'+weight_data['product_weight']+' lbs('+product_weight_kg+' kg) </td> </tr>');
                                }
                            }
                            /* if (!jQuery("#product-attribute-specs-table td:contains('Item Weight')").length) {
                                jQuery('#product-attribute-specs-table').append('<tr> <td>Item Weight</td> <td>'+weight_data['product_weight']+' lbs('+product_weight_kg+' kg) </td> </tr>');
                            } */
                        }
                        if(jQuery('.eligibility-sec').length && weight_data['product_weight'] > 1){
                            jQuery('.eligibility-sec').remove();
                        }
                        if(jQuery('.w_d_desciption').length && full_dimension !="" && full_weight !=""){
                            var dw_html = "";
                            dw_html += "Product Dimensions : <span class='d_desciption'>"+ full_dimension +"</span>";
                            dw_html += ", product weight : <span class='w_desciption'>"+ full_weight+"</span>";  
                            jQuery('.w_d_desciption').html(dw_html);
                        }
                    }                    
                } 
               
            }
        });
    }

    function getInterlinkingSection() {
        var is_brand_url = 1;
        var is_category_url = 1;
        //var brand_name = jQuery(".brand-name a:first").text();
        var brand_name = '';
        if (jQuery("#brand_name_en").length) {
            brand_name = jQuery("#brand_name_en").val();
            brand_name = brand_name.trim();
            brand_name = brand_name.toLowerCase();
        }
        if (brand_name != '') {
            is_category_url = 0;
        }

        var category = jQuery("#last_breadcrumb").val();
        var node_id = jQuery("#cat_last_node_id").val();
        if (typeof category != "undefined" && category != "") {
            category = category.trim();
        }
        if (category != '' && node_id != '') {
            category = category.toLowerCase();
            is_brand_url = 0;
        }
        if (brand_name != 'generic') {
            var interlink_url = 'https://www.ubuy.com.bd/en/ubcommon/usstore/search/interlink_brand_category';

            var linking_data = {
                lang: lang,
                pdp: 1,
                searchqtr: encodeURIComponent(brand_name),
                category: encodeURIComponent(category),
                node_id: node_id,
                is_brand_url: is_brand_url,
                is_category_url: is_category_url,
                is_arabic_store: '',
                s_id: s_id,
                product_key: entity_id,
                c_u_p_s_id:custom_url_product_section_id,
                token: csrftoken_common,
                entity_id: entity_id
            };
            linking_data = btoa(JSON.stringify(linking_data));
            jQuery.ajax({
                type: "GET",
                data: {
                    linking_data: linking_data
                },
                url: interlink_url,
                dataType: "html",
                success: function(response) {
                    jQuery("#interlinking-pdp").html(response);

                   /*  jQuery("#interlinking-pdp").find(".owl-prev").attr({
                        "aria-label": "Previous",
                        "role": "button"
                    });

                    jQuery("#interlinking-pdp").find(".owl-next").attr({
                        "aria-label": "Next",
                        "role": "button"
                    }); */
                }
            });
        }
    }


    function getCustomerReviews(customer_review_data = '', p_sku_new = '', s_sku_new = '') {
        var product_id = jQuery("#p_full_id").val();
        if (product_id == '') {
            setTimeout(function() {
                getCustomerReviews(customer_review_data);
            }, 1000);
            return;
        } 
        if(p_sku_new != "" && p_sku_new != "undefined"){
            review_query_params.pa = p_sku_new;
        }
        if(s_sku_new != "" && s_sku_new != "undefined"){
            review_query_params.sa = s_sku_new;
        }
        if(product_id != ""){
            review_query_params.pi = product_id;
        }
        if(customer_review_data != 'undefined' && customer_review_data != ''){
            review_query_params.customer_review_data = customer_review_data;
        }
        if (review_query_params.pi == "") {
            review_query_params.pi = product_id;
        }
        if (review_query_params.lng != lang) {
            review_query_params.lng = lang;
        }          
        review_query_data = btoa(unescape(encodeURIComponent(JSON.stringify(review_query_params)))); 
        jQuery.ajax({
            type: "GET",
            url: review_url,
            data: {
                p: review_query_data,
                is_one_search: is_one_search,
            },
            dataType: "html",
            beforeSend: function(xhr) {
                //showDivLoader("#product-reviews");
            },
            success: function(response) {
                //hideDivLoader("#product-reviews");
                if (review_page == '' || review_page == '1') {
                    //jQuery("#customer-rating").html(response);
                    jQuery("#pros_cons_review").html(response);
                    jQuery("#pros_cons_review").removeClass('loading');
                } else {
                    jQuery("#product-reviews").append(response);
                }
                reloadCustomerReviewNiceScroll();
                if (substorename == 'usstore') {
                    getInterlinkingSection();
                }
                getRecommendedRatingData();
            }
        });
    }

    function getCustomerReviewsByAPI(title) {
        var website_id = '70';
        var product_id = jQuery("#p_full_id").val();
        if (product_id == '') {
            setTimeout(function() {
                getCustomerReviewsByAPI(title);
            }, 1000);
            return;
        } 
        if(product_id != ""){
            review_query_params.pi = product_id;
        }
        if (review_query_params.pi == "") {
            review_query_params.pi = product_id;
        }
        if (review_query_params.lng != lang) {
            review_query_params.lng = lang;
        }          
        if(website_id != ""){
            review_query_params.website_id = website_id;
        }
        if(s_id != ""){
            review_query_params.s_id = s_id;
        }
        if(title != ""){
            review_query_params.product_title = title;
        }
        review_query_data = btoa(unescape(encodeURIComponent(JSON.stringify(review_query_params)))); 
        jQuery.ajax({
            type: "GET",
            url: review_url,
            data: {
                p: review_query_data,
                is_one_search: is_one_search,
            },
            dataType: "html",
            beforeSend: function(xhr) {
                //showDivLoader("#product-reviews");
            },
            success: function(response) {
                //hideDivLoader("#product-reviews");
                if (review_page == '' || review_page == '1') {
                    //jQuery("#customer-rating").html(response);
                    jQuery("#pros_cons_review").html(response);
                    jQuery("#pros_cons_review").removeClass('loading');
                } else {
                    jQuery("#product-reviews").append(response);
                }
                reloadCustomerReviewNiceScroll();
                if (substorename == 'usstore') {
                    getInterlinkingSection();
                }
                getRecommendedRatingData();
            }
        });
    }

    function getAIFeatures() {

        var feature_query_params = {
            sa: selected_asin,
            pa: parent_asin,
            pi: product_id,
            lng: lang,
            sname: storename,
            sbname: substorename,
            token: csrftoken_common,
            entity_id: entity_id
        }; 
        pa = jQuery("#parent_sku").val();
        
        if (pa != "") {
            feature_query_params.pa = pa;
        }
        feature_query_params = btoa(unescape(encodeURIComponent(JSON.stringify(feature_query_params)))); 
        //ai_feature_url = ai_feature_url + "?p=" + feature_query_params;
        jQuery.ajax({
            type: "GET",
            url: ai_feature_url,
            data: {
                p: feature_query_params,               
            },
            dataType: "JSON",
            success: function (response) {
                product_detail_data = false;
                if (response['quote'] != "") {
                    jQuery("#product-ai-feature").html(response['quote']);
                }

                if (response['features'] != "") {
                    jQuery("#product-ai-benefits").removeClass("loading");
                    jQuery("#product-ai-benefits").html(response['features']);
                } else {
                    jQuery("#product-ai-benefits").remove();
                }

                if (response['about_description'] != "") {
                    jQuery("#about-description").removeClass("loading");
                    jQuery("#about-description").html(response['about_description']);
                } else {
                    jQuery("#about-description").remove();
                }

                if (response['description'] != "") {
                    product_detail_data = true;
                    jQuery("#rewrite_description").removeClass("loading");
                    jQuery("#rewrite_description").html(response['description']);
                } else {
                    jQuery("#non-ai-description").removeClass("d-none");
                    jQuery("#rewrite_description").remove();
                }

                if (jQuery("#buying-guide").length) {
                    if (response['buying_guide'] != "") {
                        jQuery("#buying-guide").removeClass("loading");
                        jQuery("#buying-guide").html(response['buying_guide']);
                    } else {
                        jQuery("#buying-guide").remove();
                    }
                }
                if (product_detail_data === false) {
                    if (jQuery(".hide_product_detail_section").length > 0) {
                        jQuery(".product-detail").addClass('d-none');
                    }
                }
                
                getCustomerFaq();
            },
            error: function() {
                jQuery("#product-ai-benefits").remove();
                jQuery("#about-description").remove();
                jQuery("#non-ai-description").removeClass("d-none");
                jQuery("#rewrite_description").remove();
                jQuery("#buying-guide").remove();
                getCustomerFaq();
            }
        });
    }

    function getCustomerFaq() {

        var faq_query_params = {
            sa: selected_asin,
            pa: parent_asin,
            pi: product_id,
            pnm: product_name,
            lng: lang,
            sname: storename,
            sbname: substorename,
            entity_id: entity_id,
            token: csrftoken_common,   
            entity_id: entity_id
        }; 
        pa = jQuery("#parent_sku").val();
        
        if (pa != "") {
            faq_query_params.pa = pa;
        }
        if (debugpsr != "") {
            faq_query_params.debugpsr = debugpsr;
        }
        if (typeof faq_query_params.pnm != "undefined" && faq_query_params.pnm == "") {
            pnm = jQuery("#product_name").val();
            faq_query_params.pnm = pnm;
        }
        faq_query_params = btoa(unescape(encodeURIComponent(JSON.stringify(faq_query_params)))); 

        jQuery.ajax({
            type: "GET",
            url: faq_url,
            data: {
                p: faq_query_params
            },
            dataType: "html",
            success: function(response) {
                if (response != "") {
                    jQuery("#product-cu-c-faq").removeClass("loading");
                    jQuery("#product-cu-c-faq").html(response);
                } else {
                    jQuery("#product-cu-c-faq").remove();
                }
            }
        });
    }

    function getDescriptionData(description, short_description, title) {
        if (title != "") {
            description = "";
        }        
        if(description =="" && short_description ==""){
            return;
        }
        jQuery.ajax({
            type: "POST",
            data: {
                entity_id: entity_id,
                sname: storename,
                sbname: substorename,
                description: description,
                short_description: short_description,
                lng: lang, 
                token: csrftoken_common,                     
            },
            url: description_url,
            dataType: "JSON",
            success: function(response) {
                if (title != "") {
                    jQuery("#description").html(title);
                } else if (response['description'] !== '') {
                    jQuery("#description").html(response['description']);
                }
                if (response['short_description'] !== '') {
                    jQuery("#features").html(response['short_description']);
                }
            }
        });
    }

    function getAiDescriptionData() { 
        var product_id = jQuery("#p_full_id").val();
        if (product_id == '') {
            setTimeout(function() {
                getAiDescriptionData();
            }, 1000);
            return;
        }        
        pa = jQuery("#parent_asin").val();
        ubuy_sku = jQuery("#ubuy_sku").val();
        var desc_params = {
            sa: selected_asin,
            pa: parent_asin,
            pi: product_id,
            pnm: product_name,
            lng: lang,
            sname: storename,
            sbname: substorename,
            entity_id: entity_id,
            usku : ubuy_sku,
            token: csrftoken_common,
        };         

        if (pa != "") {
            desc_params.pa = pa;
        }
        if (typeof desc_params.pnm != "undefined" && desc_params.pnm == "") {
            pnm = jQuery("#product_name").val();
            desc_params.pnm = pnm;
        }
        if (debugpsr != "") {
            desc_params.debugpsr = debugpsr;
        }
        desc_params = btoa(unescape(encodeURIComponent(JSON.stringify(desc_params)))); 

        jQuery.ajax({
            type: "GET",
            url: ai_description_url,
            data: {
                p: desc_params
            },
            dataType: "JSON",
            success: function (response) {
                if (response['description'] != "") {
                    jQuery("#ai_description").removeClass("loading");
                    jQuery("#ai_description").html(response['description']);                    
                } else if (response['title'] != "") {
                    jQuery("#ai_description").removeClass("loading");
                    jQuery("#ai_description").html(response['title']);                    
                } else {
                    jQuery("#product-full-desc").remove();
                } 
            }
        });
    }

    function getMoreProducts(more_products, more_id, title) {
        jQuery.ajax({
            type: "POST",
            data: {
                more_products: more_products,
                s_id: s_id,
                lng: lang,
                more_id: more_id,
                title: title
            },
            url: more_product_url,
            dataType: "html",
            success: function(response) {
                jQuery("#" + more_id + " .u-loader-outer").fadeOut();
                jQuery("#" + more_id + " .u-loader").removeClass("loading");
                jQuery("#" + more_id).removeClass("u-loader-sl");
                jQuery("#" + more_id).removeClass("s1x");
                jQuery("#" + more_id).html(response);
                otherViewedProductCrowsel(more_id);
            }
        });
    }

    function getOtherStoreProduct(sku_data="") {     
        if (sku_data != "") {
            other_store_related_url +='&cp='+sku_data;
        }         
        jQuery.ajax({
            type: "GET",
            headers: {
                'Cache-Control': 'max-age=10'
            },
            url: other_store_related_url,
            dataType: "html",
            success: function (response) { 
                var responseHtml = jQuery("<div>").html(response);
                if (responseHtml.find("#Model-Product_store").length) {
                    jQuery(".fa-spin").addClass("d-none");
                    jQuery(".other_store-heading").removeClass("active");
                } else {
                    jQuery(".product-store-suggestion").hide();
                }
                jQuery(".other_store_products_section").html(response);
                var title = jQuery('.single-product-content .title').text().trim();
                if (typeof title != "undefined" && title !='') {
                    jQuery(document).find(".other_store_products_section .product-name-store").text(title);
                }
            }
        });
    }

    jQuery(document).on("click", "#add-to-cart-btn", function() {
        if (jQuery("#custom_options_form").length > 0 && jQuery("#custom_options_form").valid()) {
            reCalculatePrice();
            var custom_options = [];
            jQuery("input[data-custom-options='" + s_f + "'],select[data-custom-options='" + s_f + "']").each(function() {
                var c_opt_label = jQuery("label[for=hide_" + jQuery(this).attr('id') + "]").text().trim();
                var c_opt_name = '';
                var c_opt_value = '';
                if (jQuery(this).is('select')) {
                    c_opt_value = jQuery(this).find("option:selected").val();
                    c_opt_name = jQuery(this).find("option:selected").text().trim();
                } else {
                    c_opt_value = jQuery(this).val();
                    c_opt_name = jQuery(this).val();
                }
                custom_options.push(c_opt_label + '::' + c_opt_value + '::' + c_opt_name);
            });

            if (jQuery("#custom_select_fonts_" + s_f).length > 0) {
                jQuery('#custom_fonts').val(jQuery("#custom_select_fonts_" + s_f).find("option:selected").val());
            }
            if (jQuery("#custom_select_font_color_" + s_f).length > 0) {
                jQuery('#custom_font_color').val(jQuery("#custom_select_font_color_" + s_f).find("option:selected").val());
            }
            if (jQuery("#custom_select_surface").length > 0) {
                jQuery('#custom_surface').val(jQuery("#custom_select_surface").find("option:selected").val());
            }
            jQuery('#custom_options').val(custom_options.join("||"));
            jQuery('#custom_qty').val(jQuery("#qty").find("option:selected").val());
            jQuery("#product_addtocart_form").submit();
            jQuery("#customOptionsModal").modal('hide');
        }
    });

    function getCommonProducts(products, id, total_slides) {
        jQuery.ajax({
            type: "POST",
            data: {
                common_search: products,
                lng: lang
            },
            url: common_view_url,
            dataType: "html",
            success: function(response) {
                let currentId = '';
                jQuery("#all_common_slider").html(response);
                for (let i = 0; i < total_slides; i++) {
                    currentId = id + "_" + i;
                    jQuery("#" + currentId + " .u-loader-outer").fadeOut();
                    jQuery("#" + currentId + " .u-loader").removeClass("loading");
                    jQuery("#" + currentId).removeClass("u-loader-sl");
                    jQuery("#" + currentId).removeClass("s1x");
                    otherViewedProductCrowsel(currentId);
                }

            }
        });
    }

    function otherViewedProductCrowsel(other_id) {
        var carousel = jQuery("#" + other_id + "-carousel");
        if (carousel.length != 0) {
            var swiper = new Swiper("#" + other_id + "-carousel", {
                rtl: is_arabic_store ? true : false,
                loop: false,
                spaceBetween: 10,
                navigation: {
                    nextEl: ".product-next1",
                    prevEl: ".product-prev1"
                },
                breakpoints: {
                    0: { slidesPerView: 2 },
                    767: { slidesPerView: 3 },
                    992: { slidesPerView: 4 }
                },
                on: {
                    init: function () {
                        var slidesCount = this.slides.length - this.loopedSlides * 2;
                        var currentSlidesPerView = this.params.slidesPerView;
                        if (typeof currentSlidesPerView === 'object') {
                            var windowWidth = window.innerWidth;
                            var matched = 1; 
                            Object.keys(this.params.breakpoints).forEach(function(bp) {
                                if (windowWidth >= bp) {
                                    matched = this.params.breakpoints[bp].slidesPerView;
                                }
                            }, this);
                            currentSlidesPerView = matched;
                        }
                        if (slidesCount <= currentSlidesPerView) {
                          jQuery(this.params.navigation.nextEl).css('display', 'none');
                          jQuery(this.params.navigation.prevEl).css('display', 'none');
                        }
                    }
                }
            });
        }
    }

    function getAllStoreRelatedProducts(title = '', newSchema = false) {
        var related_product_data = {
            title: title,
            selected_asin: selected_asin,
            parent_asin: parent_asin,
            product_id: product_id,
            s_id: s_id,
            lng: lang,
            newSchema: newSchema,
            sname: storename,
            sbname: substorename,
        };
        related_product_data = btoa(JSON.stringify(related_product_data));
        jQuery.ajax({
            type: "GET",
            headers: {
                'Cache-Control': 'max-age=10'
            },
            data: {
                related_product_data: related_product_data
            },
            url: all_store_related_url,
            dataType: "html",
            success: function(response) {                
                jQuery("#all-store-related-products").html(response);
                otherViewedProductCrowsel('all-store-related-products');
            }
        });
    }

