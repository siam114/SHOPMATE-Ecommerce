            type: "GET",
            headers: {
                'Cache-Control': 'max-age=10'
            },
            url: sku_related_url,
            dataType: "html",
            success: function (response) { 
                // console.log('success');               
            }
        });
    }

    jQuery(document).on('click', '#searchRelatedProducts', function () {
        jQuery('html, body').animate({
            scrollTop: jQuery('#product-ai-feature').offset().top
        });
    });

    function searchRelatedProducts() {
            }

    function searchSimilarProducts(custom_url_replace_sync = '') {        
        query = jQuery("#searchRelatedProducts").data('q');        
        brand_name = jQuery("#searchRelatedProducts").data('b');
        var product_data = {
            q: query,
            br: brand_name,            
            lng: lang,                  
            d: device,
            sname: storename,
            sbname: substorename,
            token: csrftoken_common, 
            entity_id: entity_id,
            custom_url_replace_sync : custom_url_replace_sync,
            c_u_p_s_id : custom_url_product_section_id,
        }
        //product_data = btoa(JSON.stringify(product_data));
        product_data = btoa(unescape(encodeURIComponent(JSON.stringify(product_data))));
        ajaxRequest = jQuery.ajax({
            type: "GET",
            headers: {
                'Cache-Control': 'max-age=10'
            },
            data: {
                product_data: product_data
            },
            url: get_similar_products_url,
            dataType: "html",
            success: function (response) {
                jQuery("#search-related-product").html(response);
                /* jQuery("#search-related-product").find('.translation').remove();*/
                jQuery(".product-inner-list").addClass('col-6 mb-4'); 

                if ((response.includes('product-inner-list') == false)) {                    
                    jQuery("#search-related-products_list").remove();
                    search_button_txt = jQuery('#searchRelatedProducts').html();
                    search_button_url = base_url + 'search/?q=' + query;
                    search_button_btn = '<a href="' + search_button_url + '" class="font-normal btn btn-primary p-2 rounded pl-4 pr-4 mb-2 d-inline-block" target="_blank">' + search_button_txt + '</a>';
                    jQuery('#search-related-product-btn').find('span.font-weight-bold').html(search_button_btn);
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {

            }
        });
    }
</script>
<script>
    var max_qty = 0;
    var symbol = custom_arr_val = '';
     function showCustomForm(){
        var custom_form = 'https://www.ubuy.com.bd/en/ubcommon/common/upload_custom_data';        
        jQuery.ajax({
            type: "POST",
            data: { action: 'custom',
                custom_array : custom_arr_val,  
                symbol : symbol,
                max_qty : max_qty,
                lang_code : lang_code,
                qty_allowed: qty_allowed
             },
            url: custom_form,
            dataType: 'html',
            success: function(response) {
                jQuery('#custom_container').removeClass('loading');
                jQuery('#custom_container').html(response);
            },
            error: function(error) {
                console.log(error);
            }
        });
    }
    
    var count = 0;
    var total_custom_price = 0;
    jQuery(document).on('input change', '.custom_heading', function () {
        var element = jQuery(this);
        if (element.attr("required") || element.hasClass("imageUpload")) {
            if (element.val() !== '') {
                var id_name = element.attr('id');
                var errorElement = jQuery('#' + id_name + '_error');
                if (errorElement.length) {
                    total_custom_price = 0;
                    errorElement.html('');
                    errorElement.removeClass('error_required');
                }
            }
        }
        if (element.attr('type') === 'file') {
            var file = element[0].files[0];
            var wrapper = jQuery(this).closest('.image-group');
            var preview_img = wrapper.find('.imgPreview');
            var preview_div = wrapper.find('.img_priview');
            var input_element = jQuery(this);
            var id_name = input_element.attr('id');
            if (file) {
                var file_type = file.type;
                var file_size = file.size / (1024 * 1024);
                if (file_size > 15) {
                    preview_img.attr('src', '');
                    preview_div.addClass('d-none');
                    errorElement.html('File size must not exceed 15 MB.').addClass('error_required');
                } else if (file_type === 'image/jpeg' || file_type === 'image/png' || file_type === 'image/jpg') {
                    preview_img.attr('src', URL.createObjectURL(file));
                    preview_div.removeClass('d-none');
                    errorElement.html('').removeClass('error_required');
                    jQuery('#submit_customData').prop('disabled', false);
                } else {
                    preview_img.attr('src', '');
                    preview_div.addClass('d-none');
                    errorElement.html('File format must be JPEG, JPG or PNG.').addClass('error_required');
                }
            } else {
                preview_img.attr('src', '');
                preview_div.addClass('d-none');
                errorElement.html('').removeClass('error_required');
                jQuery('#submit_customData').prop('disabled', false);
            }
        }
        jQuery('.imageUpload').each(function () {
            var id = jQuery(this).attr('id');
            var err = jQuery('#' + id + '_error');
            if (err.length && err.hasClass('error_required') && err.html().trim() !== '') {
                jQuery('#submit_customData').prop('disabled', true);
            }
        });
    });
    jQuery(document).on('input', '.custom_heading', function () {
        var textarea = jQuery(this);
        var id = textarea.attr('id');
        var number = id.replace('text_heading', ''); // extract number (e.g., 0, 1, 2)
        var charCount = jQuery('#char-count-' + number);
        var errorSpan = jQuery('#text_heading' + number + '_error');
        var key = textarea.attr('id').replace('text_heading', '');
        var maxLength = parseInt(jQuery('#char-count-' + key).attr('maxlength'));
        var currentLength = textarea.val().length;
        if (currentLength > maxLength) {
            textarea.val(textarea.val().substring(0, maxLength));
        }
        var remaining = maxLength - currentLength;
        if (charCount.length) {
            var char_left = (remaining < 0 ? 0 : remaining);
            charCount.text(char_left + ' characters left');
        }
        if (errorSpan.length) {
            if (remaining < 0) {
                errorSpan.text('You have reached the limit!');
            } else {
                errorSpan.text('');
            }
        }
    });

    jQuery(document).on('click', '#submit_customData', function(e) {
        e.preventDefault();
        jQuery('.customize-atc-disabled, .submit-disabled').removeClass('d-none');
        var isValid = true;
        var custom_arr = [];
        total_custom_price = 0;
        let usedHeadings = new Set(); 
        jQuery("input[type='radio'].radio-custom:checked").each(function() {
            var radio = jQuery(this);
            var element_name = radio.attr('name');
            var element_value = radio.closest('label').find('.product-image-key').data('value') || radio.val();
            var imageUrl = radio.closest('label').find('img').attr('src');

            if (element_name && element_value && !usedHeadings.has(element_name)) {
                let obj = {
                    heading: element_name,
                    value: element_value
                };
                if (imageUrl) {
                    obj.image = imageUrl;
                }
                const price_match = element_value.match(/\+\s*\D{0,3}\s*(\d+)/);
                if (price_match) {
                    total_custom_price += parseInt(price_match[1]);
                }
                custom_arr.push(obj);
                usedHeadings.add(element_name);
               
            }
        });
        if (jQuery('.product-image-key').length > 0) {
            if (jQuery('.product-image-key.selected-image').length === 0) {
                var errorElement = jQuery('.select_heading_image_error');
                errorElement.html('Provide missing information');
                errorElement.addClass('error_required');
                jQuery('.customize-atc-disabled').addClass('d-none');
                isValid = false;
            }
            jQuery(document).on('click', '.product-image-key', function () {
                var container = jQuery(this).closest('.ub_form-row');  
                var errorElement = container.find('.select_heading_image_error');  
                if (jQuery(this).hasClass('selected-image')) {
                    errorElement.removeClass('error_required').html('');
                }
                isValid = true;
            });
        }
        jQuery('.custom_heading').each(function(index, element) {
            var element = jQuery(element);
            if (element.attr("required") && element.val().trim() === '') {
                var id_name = element.attr('id');
                var errorElement = jQuery('#' + id_name + '_error');
                if (errorElement.length) {
                    total_custom_price = 0;
                    errorElement.html('Provide missing information');
                    errorElement.addClass('error_required');
                    jQuery('.customize-atc-disabled').addClass('d-none');
                }
                isValid = false;
                return;
            }
            if (isValid) {
                var element_value = element.val();
                var element_name = element.attr('name');
                if (element_value !== '' && element_name !== '') {
                    if (element.attr('type') === 'file') {
                        var file = element[0].files[0];
                        if (file) {
                            custom_arr.push({
                                heading: element_name,
                                value: file.name
                            });
                        }
                    } else {
                        let clean_value = element_value
                            .replace(/\n/g, " ")
                            .replace(/'/g, "")
                            .replace(/\s+/g, " ")
                            .trim();
                        custom_arr.push({
                            heading: element_name,
                            value: clean_value
                        });
                        var match = element_value.match(/\b[A-Z]{3}\b(?=\s*\d+)/);
                        symbol = match ? match[0] : '';
                        if (element_value.includes(symbol)) {
                            const price_number = element_value.match(/\+\s*\D*(\d+)/)?.[1];
                            if (price_number) {
                                total_custom_price += parseInt(price_number);
                            }
                        }
                    }
                }
            }
        });
        if (isValid){
            jQuery("#custom_form").val(JSON.stringify(custom_arr));
            var form = document.getElementById("custom_forms");
            var formData = new FormData(form);
            custom_arr.forEach((item) => {
                var heading = item.heading.trim();
                var value = item.value;
                var image = item.image;
                var existing = formData.get(heading);
                if (image && !(existing instanceof File && existing.name)) {
                    formData.set(heading, JSON.stringify({
                        value: value,
                        image: image
                    }));
                } else if (!formData.has(heading)) {
                    formData.set(heading, value);
                }
            });
            var base_image_url = 'https://www.ubuy.com.bd/en/ubcommon/common/generate_s3_image';
            for (var pair of formData.entries()) {
                if (pair[0] === "qty") {
                    jQuery("#custom_qty").val(pair[1]);
                }
            }
            jQuery.ajax({
                url: base_image_url,
                type: 'POST',
                data: formData,
                processData: false,
                contentType: false,
                dataType: 'json',
                success: function(response) {
                    if (response.status === true) {
                        custom_arr = custom_arr.map(function(item) {
                            var item_name = item.heading.replace(/\./g, '').replace(/\s+/g, '_');
                            Object.keys(response).forEach(function(key) {
                                var custom_cdn = key.replace(/\./g, '').replace(/\s+/g, '_');
                                if (custom_cdn === item_name && typeof response[key] === 'object') {
                                    if (response[key].value) {
                                        item.value = response[key].value;
                                    }
                                    if (response[key].image) {
                                        item.image = response[key].image;
                                        var input = jQuery('[name="' + item.heading + '"]');
                                        if (input.length && input.attr('type') === 'file') {
                                            item.value = '';
                                        }
                                    }
                                }
                            });
                            return item;
                        });
                        jQuery('.customize-atc-disabled').addClass('d-none');
                        jQuery("#custom_form").val(JSON.stringify(custom_arr));
                        var old_price = parseInt('0');
                        var final_product_price = old_price;
                        jQuery("#customization_price").val(symbol + "-"+ total_custom_price);
                        jQuery('.product-price h2').html(symbol + " " + final_product_price + ' <span itemprop="price" content="' + final_product_price + '"></span>');
                        jQuery('.customize-disabled, .submit-disabled').addClass('d-none');
                        jQuery('#product_addtocart_form').submit();
                        jQuery('#customiseFormModal').modal('hide');
                    }
                },
                error: function(error) {
                    console.log(error);
                }
            });
        }
    });

    function previewImage(inputId) {
        var $input = jQuery('#' + inputId);
        var file = $input[0].files[0];
        var $previewContainer = jQuery('#image-preview-container-' + inputId);
        var $previewImage = jQuery('#image-preview-' + inputId);
        if (file && file.type.startsWith('image')) {
            var reader = new FileReader();
            reader.onload = function (event) {
                $previewImage.attr('src', event.target.result);
                $previewContainer.show();
            };
            reader.readAsDataURL(file);
        } else {
            $previewContainer.hide();
        }
    }
</script>

<script type="text/javascript">
    jQuery(document).on("click", "input[class='input_radio_mart']", function() {
        // if (!(jQuery(this).attr("checked") == "checked")) {
            var first_value = "";
            var select_values = "";
            jQuery("input[class='input_radio_mart']:checked").each(function() {
                if (select_values != '') {
                    select_values = select_values + "~" + $(this).attr("data-key");
                } else {
                    first_value = select_values = $(this).attr("data-key");
                }
            });
            combinationArr = JSON.parse(atob(combinationArr));
            if (typeof combinationArr[select_values] != "undefined" && combinationArr[select_values] != "") {
                selected_asin = combinationArr[select_values]['sku'];
            } else {
                for (var combination in combinationArr) {
                    if (combination.includes(first_value)) {
                        select_values = combination;
                        break;
                    }
                }
                if (typeof combinationArr[select_values] != "undefined" && combinationArr[select_values] != "") {
                    selected_asin = combinationArr[select_values]['sku'];
                }
            }
            showLoaderImageAbs();
            var fetch_data = {
                cur_url: current_url,
                id: entity_id,
                sku: selected_asin,
                p_sku: parent_asin,
                c_id: is_custom_id_product,
                sname: storename,
                sbname: substorename,
                is_ajax: is_ajax,
                pid: product_id,
                sid: s_id,
                can_url: canonical_url,
                lg: lang,
                dir: dir,
                fn: fn,
                d: device,
                token: csrftoken_fetch,
                lv: lv
            };

            fetch_data = btoa(JSON.stringify(fetch_data));
            jQuery.ajax({
                type: "GET",
                url: ajax_url,
                data: {
                    fetch_data: fetch_data
                },
                headers: {
                    'Cache-Control': 'max-age=10'
                },
                success: function(response, textStatus, jqXHR) {
                    if (response) {
                        jQuery('#product-view-full').html(response);                       
                        hideLoaderImageAbs();

                        if (jQuery("#shipping_discount").length > 0 && jQuery("#progress-bar-container").length <= 0 && typeof checkShppingDiscountEligible == "function") {
                            checkShppingDiscountEligible();
                        }
                    }
                    if (jqXHR.status === 204) {
                        window.location.reload();
                    }
                }
            });
        // }
    });
</script>
