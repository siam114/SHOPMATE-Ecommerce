    function getAddWarrantyPage(price, symbol) {
        warranrt_url = 'https://www.ubuy.com.bd/en/ubcommon/add-warranty-v4';
        var membership_item = true;
        var customer_id = '';
        if (device == 'm') {
            warranrt_url = 'https://www.ubuy.com.bd/en/ubcommon/add-warranty-mobile-v4';
        }
        jQuery.ajax({
            type: "GET",
            url: warranrt_url,
            data: {
                price: price,
                symbol: symbol,
                membership_item: membership_item,
                customer_id:customer_id,
                token: csrftoken_common, 
                entity_id: entity_id  
            },
            success: function(resp) {
                jQuery(".select-warranty-options").after(resp);
                jQuery(".select-warranty-options").find('#selected_plan').find(".span_spinner").remove();
                jQuery(".select-warranty-options").find('#selected_plan').removeClass('loading');
            },
        });
    }

    function wishlistCheck(customer_id = '', custom_selected_asin = '') {
        var customer_id = '';
        if(customer_id ==''){
           return;
        }
        var variation_sku = jQuery('#asin').val();
        var parent_sku = jQuery("#parent_asin").val();
        var fetchData = {
            variation_sku: custom_selected_asin != '' ? custom_selected_asin : variation_sku,
            parent_sku: parent_sku,token: csrftoken_common,
            entity_id: entity_id            
        };
        jQuery.ajax({
            type: "POST",
            data: fetchData,
            url: wishlist_check_url,
            success: function(response) {
                var res = JSON.parse(response);
                if(res.status == true){
                    jQuery(".add-to-wishlist").addClass("active");
                    jQuery(".add-to-wishlist").html('<i class="fa-heart fas"></i>');
                }else{
                    jQuery(".add-to-wishlist").removeClass("active");
                    jQuery(".add-to-wishlist").html('<i class="fa-heart far"></i>');
                }
            },
            error: function(error) {
            }
        });
    }

    function getVariations(variations, sizeChart, variationsRelatedData = '') {
        if (custome_ajax_var_request) {
            var var_url = 'https://www.ubuy.com.bd/en/ubcommon/mart/detail-v4/variations';
        }
        jQuery.ajax({
            type: "POST",
            data: {
                variations: variations,
                selected_asin: selected_asin,
                s_id: s_id,
                lang: lang,
                sizeChart: sizeChart,
                url_key: url_key,
                variationsRelatedData: variationsRelatedData,
            },
            url: var_url,
            dataType: "html",
            success: function(response) {
                jQuery("#product-variations").removeClass('loading');
                jQuery("#product-variations").html(response);
                jQuery("#variation-mobile").html(response);
                if (substorename == "used") {
                    //Only for US (ebay store)
                    var first_variation = jQuery(".veriations-options:first").find(".input_radio_used:first");
                    first_variation.click();
                }else if(substorename == "useduk"){
                    //Only for UK (ebay store)
                    var first_variation = jQuery(".veriations-options:first").find(".input_radio_useduk:first");
                    first_variation.click();
                }else if(substorename == "usedde"){
                    //Only for DE (ebay store)
                    var first_variation = jQuery(".veriations-options:first").find(".input_radio_usedde:first");
                    first_variation.click();
                }else if (substorename == "samsclub") {
                    if(typeof setFirstVariationSamsclub === 'function' && !is_frist_click){
                        is_frist_click = true;
                        setFirstVariationSamsclub();
                    }
                }else if (substorename == "cn1688") {
                    jQuery('.quantity-input .minus-btn').click();
                    var first_variation = jQuery(".veriations-options:first").find(".input_radio_cn1688:first");
                    first_variation.click();
                }else if (substorename == "hk1688") {
                    jQuery('.quantity-input .minus-btn').click();
                    var first_variation = jQuery(".veriations-options:first").find(".input_radio_hk1688:first");
                    first_variation.click();
                }else if(substorename == "newegg"){
                    isVariationOpen = true;
                    var variationNodeList = document.querySelectorAll('#product-variations select');
                    for (var variationNode of variationNodeList) {
                        var variationOptionNodeList = variationNode.querySelectorAll('option:disabled');
                        for (var variationOptionNode of variationOptionNodeList) {
                            var optionAsin = variationOptionNode.getAttribute('asin');
                        }
                    }
                }else if(substorename == "inny"){
                    var first_variation = jQuery(".veriations-options:first").find(".input_radio_inny:first");
                    first_variation.click();
                }
                if (!is_reloaded) {
                    is_reloaded = true;
                    // jQuery('select[id^=select_]').first().trigger('change');
                }
            }
        });
    }
    
    function getSizeChart(id, dept) {
        if (id == "size-chart-body") {
            jQuery('#sizechartModal').modal('show');
        }
        if (custome_ajax_schart_request) {
            var size_chart_url = 'https://www.ubuy.com.bd/en/ubcommon/mart/detail-v4/size-chart';
        }
        jQuery.ajax({
            type: "POST",
            url: size_chart_url,
            data: {
                params: jQuery("#size_chart_details").val(),
                dept: dept,
                s_id: s_id,
                type: size_chart_params,
                lang: lang
            },
            success: function(size_chart) {
                if (id == "size-chart-body") {
                    if (size_chart) {
                        jQuery("#" + id).html(size_chart);
                        jQuery("#tabContent").attr("id", "dept0-tab");
                        jQuery("#dept0-tab").addClass("a-loaded");
                    } else {
                        jQuery("#" + id).html(size_chart_notavailable);
                    }
                } else {
                    jQuery('div[class="modal-body"]').html(size_chart);
                }
                jQuery(".a-icon-extender-expand").attr("class", "fas fa-angle-down");
                jQuery("#kid #kidLink").contents().unwrap();
            }
        });
    }

    function getRecommendedRatingData() {        
        var product_id = jQuery("#p_full_id").val();
        if (product_id == '') {
            setTimeout(function() {
                getRecommendedRatingData();
            }, 1000);
            return;
        }
        avg_rating = "";
        if(jQuery('#review_avg').length){
            avg_rating = jQuery('#review_avg').val();
        }
        respondents_url = 'https://www.ubuy.com.bd/en/ubcommon/recommended-rating';

        var respondents_query_params = {            
            pi: product_id, 
            lng: lang,          
            d: device,            
            sname: storename,
            sbname: substorename,
            token: csrftoken_common,
            entity_id: entity_id,
            avg_rating : avg_rating
        }

        if(debugpsr != ""){
            respondents_query_params.debugpsr = debugpsr;
        }
        respondents_query_params = btoa(JSON.stringify(respondents_query_params));
       
        jQuery.ajax({
            type: "GET",
            url: respondents_url,
            data: {
                p: respondents_query_params
            },
            success: function (resp) {
                jQuery("#recommendedRating").html(resp);                
            },
        });
    }
    function getGloberProduct(data="") {
        jQuery("#product-cu-c-globel").removeClass("loading");
        jQuery("#product-cu-c-globel").html('');
        return false;
        if (data != "") {
            global_related_url +='&cp='+data;
        }
        if (debugpsr != "") {
            global_related_url +='&debugpsr=' + debugpsr;
        }
        jQuery.ajax({
            type: "GET",
            headers: {
                'Cache-Control': 'max-age=10'
            },
            url: global_related_url,
            dataType: "html",
            success: function (response) { 
                jQuery("#product-cu-c-globel").removeClass("loading");                
                jQuery("#product-cu-c-globel").html(response);               
            }
        });
    }

